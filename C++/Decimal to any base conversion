#include<iostream>

#include<cstring>

using namespace std;



int main()
{
    int t;
    cin>>t;
    while(t--)
    {
        int b,n;
        cin>>b>>n;
        char s[100000];
        int i=0;
        
        while(n>0)
        {
           
    
            if(n%b >= 10)
            {
               s[i] = n%b - 10 + 'A';
            }
    
            else
            {
               s[i] = n%b + '0';
            }
            
            n /= b;
            i++;
    
            
        }
        s[i] = '\0';
        for(int i = strlen(s) - 1;i>=0;i--)
        {
            cout<<s[i];
        }
        cout<<endl;
        
        
        
    }
}
