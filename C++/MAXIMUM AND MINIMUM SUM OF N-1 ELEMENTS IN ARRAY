#include<iostream>
using namespace std;

int main()
{
    long int a[5],temp,sumMin=0,sumMax=0;
    
    for(int i=0;i<5;i++)
    {
        cin>>a[i];
    }
    
    for(int i=0;i<5;i++)
    {
        for(int j=i+1;j<5;j++)
        {
            if(a[i]>a[j])
            {
                temp=a[i];
                a[i]=a[j];
                a[j]=temp;
            }
        }
    }
    
    for(int i=0;i<4;i++)
    {
        sumMin+=a[i];
    }
    cout<<sumMin<<" ";
    
    for(int i=1;i<5;i++)
    {
        sumMax+=a[i];
    }
    cout<<sumMax;
}
