#include<iostream>
using namespace std;

 int main(int argc, char const *argv[])
 {
     int c=0,n;
     int a=0,b=1;
     
     cout<<"Enter a number to generate Fibonacci series for first n terms\n";
     cin>>n;  // Taking Input n for Fibonacci Sequence

     cout<<"Fibonacci series for first "<<n<<" Terms : \n";
     for(int i=0; i<n;i++){

         cout<<c<<" ";
         a=b; // Logic for getting Fibonacci Sequence
         b=c;
         c=a+b;
     }

     return 0;
 }
 
